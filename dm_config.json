{
  "commands": {
    "register": {
      "type": "process",
      "args": [],
      "stack": [
        "_select_guild", "_select_type", "_set_new_key", "_set_new_value"
      ]
    },
    "_select_guild": {
      "type": "request",
      "prompt": "Which guild are you configuring?",
      "args": [
        "guild"
      ],
      "validation": "guild"
    },
    "_select_type": {
      "type": "request",
      "prompt": "Which type are you configuring?",
      "args": [
        "type"
      ],
      "validation": "type"
    },
    "_set_new_key": {
      "prompt": "Enter a key",
      "args": [
        "key"
      ]
    },
    "_set_new_value": {
      "prompt": "Enter a value",
      "args": [
        "value"
      ]
    },
    "acknowledgement": {
      "response": "",
      "args": [

      ]
    },
    "guilds": {
      "type": "Information",
      "response": "Guilds that you are an administrator of: \n%s",
      "response_args": [
        "guilds"
      ]
    },
    "members": {
      "type": "Information",
      "response": "Members that are in %s: \n%s",
      "response_args": [
        "users"
      ]
    }
  }
}